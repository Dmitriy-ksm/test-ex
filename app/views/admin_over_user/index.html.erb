<div>
    <h1>Users</h1>
    <table>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th></th>
      </tr>
      <% @users.each do |user| %>
        <tr>
          <td>
            <%= user.first_name %>
          </td>
          <td>
            <%= user.last_name %>
          </td>
          <td>
            <%= link_to "Редактировать", admin_user_edit_path(user) %>
          </td>
        </tr>
      <% end %>
  </table>
</div>
